<%#= stylesheet_link_tag "ird" %>
<html>
    <head>
        <title>Hotels</title>
        <meta charset="UTF-8">
       
      

        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <section class="hotel-wrapper">
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <!-- add category button -->
                <div class="category">
                  <h4 class="category-title">Beverages</h4>
                 <span data-bs-toggle="modal" data-bs-target="#addcategoryModal"><img style="cursor: pointer; margin: 10px;"src="https://sit.jhes.cms.jio.com/jiohotels/images/users/add-property-icon.svg" alt="Add Category"></span>
                </div>
                <!-- Add dishes button -->
                <div class="dishes">
                <h1 class="dishes-title">Add more dishes</h1>
                 <span data-bs-toggle="modal" data-bs-target="#adddishesModal" style="cursor: pointer;"><img style=" margin: 10px;"src="https://sit.jhes.cms.jio.com/jiohotels/images/users/add-property-icon.svg" alt="Add Dishes"></span>
                </div>
              </div>
            </div>
          </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-4">
                        <div class="card">
                            <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/8/40428/3d1156a7cc37fda7f5b0761d23e2575a_o2_featured_v2.jpg" alt="Card image" style="width:100%">
                            <div class="card-body">
                              <div class="row">
                                <div class="col-md-6">
                                  <h4 class="card-title">Masala Dosa</h4>
                                </div>
                                <div class="col-md-6">
                                  <div class="card-main-head">
                                    <p class="veg-text"><img class="veg-img1" src="img/veg.png">
                                    <span class="star-text">Veg</span></p>
                                  </div>
                                </div>
                              </div>
                              
                              <div class="card-body-text">
                                <p class="card-text">South Indian, North Indian, Sandwich, Street Food, Desserts, Beverages </p>
                                <p class="card-text">₹200</p>
                                
                              </div>
                              
                            </div>
                          </div>
                    </div>
                    <div class="col-md-4">
                      <div class="card">
                        <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/4/19637714/fd60f65a6d4f3e6099333a908dce7d6d_o2_featured_v2.jpg" alt="Card image" style="width:100%">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-md-6">
                              <h4 class="card-title">Ice Cream</h4>
                            </div>
                            <div class="col-md-6">
                              <div class="card-main-head">
                                <p class="veg-text"><img class="veg-img1" src="img/veg.png">
                                <span class="star-text">Veg</span></p>
                              </div>
                            </div>
                          </div>
                          
                          <div class="card-body-text">
                            <p class="card-text">Ice Cream, Desserts</p>
                            <p class="card-text">₹150</p>
                            
                          </div>
                          
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="card">
                        <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/chains/2/48422/9dec895236797ec9720dc72a9a20812a_o2_featured_v2.jpg" alt="Card image" style="width:100%">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-md-6">
                              <h4 class="card-title">Cake</h4>
                            </div>
                            <div class="col-md-6">
                              <div class="card-main-head">
                                <p class="veg-text"><img class="veg-img1" src="img/veg.png">
                                <span class="star-text">Veg</span></p>
                              </div>
                            </div>
                          </div>
                          
                          <div class="card-body-text">
                            <p class="card-text">Bakery, Desserts</p>
                            <p class="card-text">₹50</p>
                            
                          </div>
                          
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="card">
                        <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/5/18603575/4eba6488e0ce5ffca7e0323427ee087d_o2_featured_v2.jpg" alt="Card image" style="width:100%">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-md-6">
                              <h4 class="card-title">Juices</h4>
                            </div>
                            <div class="col-md-6">
                              <div class="card-main-head">
                                <p class="veg-text"><img class="veg-img1" src="img/veg.png">
                                <span class="star-text">Veg</span></p>
                              </div>
                            </div>
                          </div>
                          
                          <div class="card-body-text">
                            <p class="card-text">Juices, Shake, Ice Cream, Fast Food</p>
                            <p class="card-text">₹150</p>
                            
                          </div>
                          
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="card">
                        <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/chains/2/19726202/d104a004a202e3e0db28d70c04331c98.jpg?output-format=webp&fit=around|771.75:416.25&crop=771.75:416.25;*,*" alt="Card image" style="width:100%">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-md-6">
                              <h4 class="card-title">Fried Chicken</h4>
                            </div>
                            <div class="col-md-6">
                              <div class="card-main-head">
                                <p class="veg-text"><img class="veg-img1" src="img/New-Non-Logo.png">
                                <span class="star-text-non-veg">Non-veg</span></p>
                              </div>
                            </div>
                          </div>
                          
                          <div class="card-body-text">
                            <p class="card-text">Asian, Korean, Chinese</p>
                            <p class="card-text">₹300</p>
                            
                          </div>
                          
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="card">
                        <img class="card-img-top" src="https://b.zmtcdn.com/data/pictures/6/36446/ee151552b4da269139ae5a8d765751ce.jpg?output-format=webp&fit=around|771.75:416.25&crop=771.75:416.25;*,*" alt="Card image" style="width:100%">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-md-6">
                              <h4 class="card-title">Chicken Momos</h4>
                            </div>
                            <div class="col-md-6">
                              <div class="card-main-head">
                                <p class="veg-text"><img class="veg-img1" src="img/New-Non-Logo.png">
                                <span class="star-text-non-veg">Non-Veg</span></p>
                              </div>
                            </div>
                          </div>
                          
                          <div class="card-body-text">
                            <p class="card-text">Chinese, Seafood, Cantonese, Asian, Thai</p>
                            <p class="card-text">₹300</p>
                            
                          </div>
                          
                        </div>
                      </div>
                    </div>
                </div>
            </div>
        </section>
    </body>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>

    <!-- Add Category Modal -->
    <div class="modal fade" id="addcategoryModal" tabindex="-1" aria-labelledby="addcategoryModalLabel" aria-modal="true" role="dialog">
      <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
          <div class="modal-header">
            <h2 class="modal-title text-center" id="staticBackdropLabel"> Add New Category </h2>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <div class="add-prop-box add-prop-box">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                  <form onsubmit="return false" autocomplete="off">
                    <div class="user-box">
                        <input id="category_name" type="text" name="user-name" maxlength="20" required="">
                      <label> Category Name</label>
                    </div>
                  </form>
                </div>
                <div class="prop-button-category text-center mt-2">
                  <button id="save_category" class="btn btn-modal-solid">Save</button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Add Dishes modal -->
    <div class="modal fade" id="adddishesModal" tabindex="-1" aria-labelledby="adddishesModalLabel" aria-modal="true" role="dialog">
      <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
          <div class="modal-header">
            <h2 class="modal-title text-center" id="staticBackdropLabel"> Add New Dish </h2>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body" style="padding-top: 0;">
            <div class="add-prop-box add-prop-box">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                  <form onsubmit="return false" autocomplete="off">
                    <div class="dish-form">
                      <h4 class="upload-img-text">Upload Image</h4>
                      <div class="card1 content-card customized-notification-card ">
                        <div class="card-content">
                          <div class="upload-buttons">
                          <label for="image_uploaded_image" class="btn btn-skip">Choose File</label>
                          <input class="btn btn-skip1" data-image-access="image_url" type="file" name="image[uploaded_image]" id="image_uploaded_image">
                          </div>
                          <span class="content_alert" id="content_alert_background"></span>
                        </div>
                      </div>
                    </div>
                    <div class="user-box">
                        <input id="dish_name" type="text" name="user-name" maxlength="20" required="">
                      <label> Dish Name</label>
                    </div>
                    <div class="user-box">
                      <input id="dish_desc" type="text" name="user-name" maxlength="100" required="">
                      <label> Dish Description</label>
                    </div>
                    <div class="form-group">
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-check form-check-inline">
                            <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="option1" />
                            <label class="form-check-label veg-check" for="inlineCheckbox1"> Veg</label>
                            <img class="veg-img" src="img/veg.png"> 
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-check form-check-inline">
                            <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2" />
                            <label class="form-check-label veg-check" for="inlineCheckbox2">Non-veg</label>
                            <img class="veg-img" src="img/New-Non-Logo.png">
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="user-box">
                      <input id="dish_desc" type="text" name="user-name" maxlength="100" required="">
                      <label> Dish Price</label>
                    </div>
                  </form>
                </div>
                <div class="prop-button-category text-center mt-2">
                  <button id="save_category" class="btn btn-modal-solid">Save</button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</html>